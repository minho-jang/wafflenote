{
  "name": "Waffle Note",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Waffle Note",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "permissions": [
    "storage",
    "tabs",
    "desktopCapture",
    "activeTab",
    "declarativeContent",
    "tabCapture",
    "downloads",
    "file://*/*",
    "<all_urls>",
    "unlimitedStorage"
  ],
  "icons": {},
  "browser_action": {
    "default_icon": "WaffleLogo.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://unpkg.com https://cdnjs.cloudflare.com ; object-src 'self'"
}
